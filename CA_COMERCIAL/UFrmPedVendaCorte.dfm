  object qryPesq: TOraQuery
    Session = FrmPrincipal.DB
    SQL.Strings = (
      'SELECT'
      '  PEDIDO_VENDA.ID_VENDEDOR,'
      '  VENDEDOR.NM_VENDEDOR,'
      '  Material_embalagem.ID_PRODMATEEMBA,'
      '  Material_embalagem.NM_PRODMATEEMBA,'
      '  sum(VL_DIFERENCA) AS diferenca,'
      
        '  Sum(PEDIDO_VENDA_ITEM.VL_UNITITEMPEDIVEND * PEDIDO_VENDA_ITEM.' +
        'QN_PESOITEMPEDIVEND) AS TOTAL'
      'FROM'
      '  PEDIDO_VENDA_ITEM,'
      '  PEDIDO_VENDA,'
      '  PEDIDO_VENDA_TIPO,'
      '  VENDEDOR,'
      '  MATERIAL_EMBALAGEM,'
      '  MATERIAL'
      'WHERE'
      '  (PEDIDO_VENDA_ITEM.ID_PEDIVEND = PEDIDO_VENDA.ID_PEDIVEND)'
      '  AND (PEDIDO_VENDA.ID_VENDEDOR = VENDEDOR.ID_VENDEDOR)'
      
        '  AND (PEDIDO_VENDA.ID_TIPOPEDIVEND = PEDIDO_VENDA_TIPO.ID_TIPOP' +
        'EDIVEND)'
      
        '  AND (Pedido_venda_item.ID_MATEEMBA = Material_embalagem.ID_MAT' +
        'EEMBA)'
      '  AND MATERIAL.ID_MATERIAL = Pedido_venda_item.ID_MATERIAL'
      '  AND NVL(PEDIDO_VENDA.FL_CORTE,'#39'N'#39') = '#39'N'#39
