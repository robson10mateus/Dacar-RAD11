      '  AND (Material_embalagem.ID_EMBALAGEM = Embalagem.ID_EMBALAGEM)'
      '  AND MATERIAL.ID_MATERIAL = Pedido_venda_item.ID_MATERIAL'
      '  AND (PEDIDO_VENDA_ITEM.ID_PEDIVEND = PEDIDO_VENDA.ID_PEDIVEND)'
      
        '  AND (PEDIDO_VENDA.ID_TIPOPEDIVEND = PEDIDO_VENDA_TIPO.ID_TIPOP' +
        'EDIVEND)'
      
        '  AND (MATERIAL_EMBALAGEM.ID_MATEEMBA = MATERIAL_EMBALAGEM_COMPL' +
        'EMENTO.ID_MATEEMBA)'
      '  AND NVL(PEDIDO_VENDA.FL_CORTE,'#39'N'#39') = '#39'N'#39
      '  AND (PEDIDO_VENDA.FL_STATPEDIVEND = '#39'AB'#39')'
      '  AND trunc(PEDIDO_VENDA.DT_PEDIVEND) = :P_DATA'
      '  AND (PEDIDO_VENDA.ID_VENDEDOR = :P_VENDEDOR )'
