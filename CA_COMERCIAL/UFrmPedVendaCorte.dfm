      '  pedido_venda_item.QN_EMBAITEMPEDIVEND,'
      '  Material_embalagem_complemento.QN_TARAEMBAPROD,'
      '  DECODE(NVL(Pedido_venda_item.QN_EMBAITEMPEDIVEND,0), 0, 0, '
      
        '        (Pedido_venda_item.QN_PESOITEMPEDIVEND / Pedido_venda_it' +
        'em.QN_EMBAITEMPEDIVEND)) QN_PESOPADRITEMPEDIVEND'
      'FROM'
      '  PEDIDO_VENDA,'
      '  PEDIDO_VENDA_TIPO,'
      '  PEDIDO_VENDA_ITEM,'
      '  MATERIAL_EMBALAGEM,'
      '  EMBALAGEM,'
      '  MATERIAL,'
      '  MATERIAL_EMBALAGEM_COMPLEMENTO'
      'WHERE'
      
        '  (Pedido_venda_item.ID_MATEEMBA = Material_embalagem.ID_MATEEMB' +
