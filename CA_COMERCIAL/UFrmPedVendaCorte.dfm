  object qryPedExc: TOraQuery
    Session = FrmPrincipal.DB
    SQL.Strings = (
      'SELECT'
      '  pedido_venda_item.ID_PEDIVEND,'
      '  Pedido_venda_item.id_itempedivend,'
      '  Material_embalagem.ID_PRODMATEEMBA,'
      '  Pedido_venda_item.VL_DIFERENCA,'
