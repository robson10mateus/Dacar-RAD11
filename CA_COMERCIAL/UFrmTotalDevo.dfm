  object qryTotaDevo: TOraQuery
    Session = FrmPrincipal.DB
    SQL.Strings = (
      
        'SELECT SUM( (NVL(Pedido_devolucao_venda_item.QN_PESOITEMPEDIDEVO' +
        'VEND,0) * NVL(Pedido_venda_item.VL_UNITITEMPEDIVEND,0)) ) VL_TOT' +
        'ADEVO,'
      
        '       SUM( ( NVL(QN_PSREEXPEITEMPEDIDEVOVEND,0) - NVL(QN_TRREEX' +
        'PEITEMPEDIDEVOVEND,0)) * '
      
        '              NVL(Pedido_venda_item.VL_UNITITEMPEDIVEND,0) ) VL_' +
        'DEVEXPE,'
      
        '       SUM( (NVL(Pedido_devolucao_venda_item.QN_PESOQUEBITEMPEDI' +
        'DEVOVEND,0) * NVL(Pedido_venda_item.VL_UNITITEMPEDIVEND,0)) ) VL' +
        '_TOTAQUEB,'
      '       SUM('
      '       CASE WHEN  NVL(QN_PSREEXPEITEMPEDIDEVOVEND,0) > 0 THEN'
      
        '              ( NVL(QN_PSREEXPEITEMPEDIDEVOVEND,0) - NVL(QN_TRRE' +
        'EXPEITEMPEDIDEVOVEND,0)) * '
      
        '                    NVL(Pedido_venda_item.VL_UNITITEMPEDIVEND,0)' +
        ' '
      '       ELSE              '
      
        '             (NVL(Pedido_devolucao_venda_item.QN_PESOITEMPEDIDEV' +
        'OVEND,0) * NVL(Pedido_venda_item.VL_UNITITEMPEDIVEND,0)) '
      '       END +'
      
        '            (NVL(Pedido_devolucao_venda_item.QN_PESOQUEBITEMPEDI' +
        'DEVOVEND,0) * NVL(Pedido_venda_item.VL_UNITITEMPEDIVEND,0)) ) VL' +
        '_TOTAOPER'
      ''
      '  FROM PEDIDO_DEVOLUCAO_VENDA Pedido_devolucao_venda,'
      '       PEDIDO_DEVOLUCAO_VENDA_ITEM Pedido_devolucao_venda_item,'
      '       PEDIDO_VENDA_ITEM Pedido_venda_item'
      
        ' WHERE (Pedido_devolucao_venda.ID_PEDIDEVOVEND = Pedido_devoluca' +
        'o_venda_item.ID_PEDIDEVOVEND)'
      
        '   AND (Pedido_devolucao_venda_item.ID_MATEEMBA = Pedido_venda_i' +
        'tem.ID_MATEEMBA)'
      
        '   AND (Pedido_devolucao_venda.ID_PEDIVEND = Pedido_venda_item.I' +
        'D_PEDIVEND)'
      
        '   AND ((Pedido_devolucao_venda.ID_PEDIDEVOVEND = :ID_PEDIDEVOVE' +
        'ND))')
    Left = 80
    Top = 200
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ID_PEDIDEVOVEND'
        Value = nil
      end>
