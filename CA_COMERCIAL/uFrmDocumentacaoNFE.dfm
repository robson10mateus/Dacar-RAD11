  object qryCargaPedidos: TOraQuery
    LocalUpdate = True
    Session = FrmPrincipal.DB
    SQL.Strings = (
      
        'SELECT DISTINCT C.ID_CARGEXPE, PV.ID_PEDIVEND, NF.NR_NOTAFISC, C' +
        'L.ID_CLIENTE, CL.NM_CLIENTE, PV.ID_VENDEDOR, V.NM_VENDEDOR, VC.I' +
        'D_VALE'
      'FROM EXPEDICAO_CARGA_ITEM CI'
      'INNER JOIN EXPEDICAO_CARGA C ON (C.ID_CARGEXPE = CI.ID_CARGEXPE)'
      
        'INNER JOIN PEDIDO_VENDA PV ON (PV.ID_PEDIVEND = CI.ID_PEDIVEND A' +
        'ND PV.FL_PNF = '#39'N'#39')'
      
        'INNER JOIN PEDIDO_VENDA_NOTA_FISCAL PVN ON (PVN.ID_PEDIVEND = PV' +
        '.ID_PEDIVEND)'
      'INNER JOIN NOTA_FISCAL NF ON (NF.ID_NOTAFISC = PVN.ID_NOTAFISC)'
      'INNER JOIN VENDEDOR V ON (V.ID_VENDEDOR = PV.ID_VENDEDOR)'
      'INNER JOIN CLIENTE CL ON (CL.ID_CLIENTE = PV.ID_CLIENTE)'
      'LEFT JOIN VALE_CAIXA VC ON (VC.ID_PEDIDO = PV.ID_PEDIVEND)'
      
        'GROUP BY C.ID_CARGEXPE, PV.ID_PEDIVEND, NF.NR_NOTAFISC, CL.ID_CL' +
        'IENTE, CL.NM_CLIENTE, PV.ID_VENDEDOR, V.NM_VENDEDOR, VC.ID_VALE'
      'ORDER BY PV.ID_PEDIVEND  ')
    MasterSource = dtsCargas
    FetchAll = True
    CachedUpdates = True
    Left = 133
    Top = 9
    object qryCargaPedidosID_PEDIVEND: TFloatField
      FieldName = 'ID_PEDIVEND'
      Required = True
