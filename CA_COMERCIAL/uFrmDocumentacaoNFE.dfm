  object qryCargas: TOraQuery
    Session = FrmPrincipal.DB
    SQL.Strings = (
      
        'SELECT DISTINCT CA.ID_CARGEXPE, CA.GN_PLACVEICTRAN, SUBSTR(CA.GN' +
        '_PLACVEICTRAN,1,3)||'#39' - '#39'||SUBSTR(CA.GN_PLACVEICTRAN,4,4) AS PLA' +
        'CA, TV.ID_VEICTRAN,'
      
        'TV.NR_ULTIMO_KM, CA.MOTORISTA, PV.DT_ENTRPEDIVEND, CA.QT_PESOBAL' +
        'ANCAO, COUNT(DISTINCT PV.ID_PEDIVEND) AS TT_PEDIDO, SUM(CI.QN_EM' +
        'BAEXPEITEMCARGEXPE) AS TT_CAIXAS,'
      'CA.ID_MOTORISTA'
      'FROM EXPEDICAO_CARGA_ITEM CI'
      
        'INNER JOIN EXPEDICAO_CARGA CA ON (CA.ID_CARGEXPE = CI.ID_CARGEXP' +
        'E)'
      'INNER JOIN PEDIDO_VENDA PV ON (PV.ID_PEDIVEND = CI.ID_PEDIVEND)'
      
        'INNER JOIN PEDIDO_VENDA_NOTA_FISCAL PVN ON (PVN.ID_PEDIVEND = PV' +
        '.ID_PEDIVEND)'
      'INNER JOIN NOTA_FISCAL NF ON (NF.ID_NOTAFISC = PVN.ID_NOTAFISC)'
      
        'INNER JOIN TRANSPORTADOR_VEICULO TV ON (TV.GN_PLACVEICTRAN = CA.' +
        'GN_PLACVEICTRAN)'
      
        'GROUP BY CA.ID_CARGEXPE, CA.GN_PLACVEICTRAN, TV.ID_VEICTRAN, TV.' +
        'NR_ULTIMO_KM, CA.MOTORISTA, CA.ID_MOTORISTA, PV.DT_ENTRPEDIVEND,' +
        ' CA.QT_PESOBALANCAO')
    Left = 204
    Top = 9
