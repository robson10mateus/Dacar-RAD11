    SQL.Strings = (
      'Select C.ID_Base , U.ID_USUARIO,C.Descr,'
      '           U.LOG_USUARIO, C.usuario ,'
      
        '       SUBSTR( SUBSTR(C.Conexao, INSTR(C.Conexao, '#39'@'#39') + 1 , LEN' +
        'GTH(C.Conexao) ), 1,'
      
